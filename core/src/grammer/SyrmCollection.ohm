SyrmCollection <: SyrmCascade {
  /* override */
  space := whitespace | lineTerminator
  
  /* root */
  Root := "<Collection>" Def<pascalCase>* "</Collection>"
  
  /* -------------------------------------------------------------------------- */
  /* def                                                                        */
  /* -------------------------------------------------------------------------- */
  
  Def<TagName> = "<" TagName ">" space* Block* space* "</" TagName ">"
  
  /* -------------------------------------------------------------------------- */
  /* property name                                                              */
  /* -------------------------------------------------------------------------- */
  
  propertyName := kebabCase
  
  /* -------------------------------------------------------------------------- */
  /* property Value                                                             */
  /* -------------------------------------------------------------------------- */
  
  PropertyValue := propsFunc | PropertyValueFunc | kebabCase | Formula
}