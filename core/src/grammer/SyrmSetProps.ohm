SyrmCollection <: SyrmCascade {
  /* root */
  Root := "<SetProps>" Def<pascalCase>* "</SetProps>"
  
  /* -------------------------------------------------------------------------- */
  /* block                                                                      */
  /* -------------------------------------------------------------------------- */
  
  /* 
    @diff
      - No suffix block defined 
  */
  Block := 
    | DeclarationsBlockWithPrefix syrmBetweenAnnotation DeclarationsBlock -- between
    | DeclarationsBlockWithPrefix -- prefixed
    | DeclarationsBlock -- basic
  
  /* -------------------------------------------------------------------------- */
  /* property name                                                              */
  /* -------------------------------------------------------------------------- */
  
  /* 
    @diff
      - Collection calls are not allowed
      - Kebab case not Pascal case
  */
  propertyName := pascalCase
  
  /* -------------------------------------------------------------------------- */
  /* property Value                                                             */
  /* -------------------------------------------------------------------------- */
  
  /*
    @diff
      - Collection keywords are not available.
      - Kebab case not Pascal case
  */
  PropertyValue := propsFunc | PropertyValueFunc | pascalCase | Formula
}