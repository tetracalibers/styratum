DeclarationBlock <: SyrmedCssInterface {
  
  /* -------------------------------------------------------------------------- */
  /* WAI-ARIA                                                                   */
  /* -------------------------------------------------------------------------- */
  
  AriaDeclarationBlock = "{" AriaDeclaration "}"
  AriaDeclaration = ariaLabel ":" ariaValue ";"
  
  /* -------------------------------------------------------------------------- */
  /* CSS                                                                        */
  /* -------------------------------------------------------------------------- */
  
  DeclarationBlock = "{" Declaration+ "}"
  Declaration = PropertyName ":" PropertyValue+ ";"
}