SyrmCollection <: SyrmCascade {
  /* root */
  Root := "<Collection>" Def<pascalCase>* "</Collection>"
  
  /* -------------------------------------------------------------------------- */
  /* def                                                                        */
  /* -------------------------------------------------------------------------- */
  
  Def<TagName> = "<" TagName ">" space* Block* space* "</" TagName ">"
  
  /* -------------------------------------------------------------------------- */
  /* block                                                                      */
  /* -------------------------------------------------------------------------- */
  
  /* 
    @diff
      - DeclarationsBlockWithPrefix DeclarationsBlock* syrmSuffixAnnotation
  */
  Block := 
    | DeclarationsBlockWithPrefix syrmBetweenAnnotation DeclarationsBlock -- between
    | DeclarationsBlockWithPrefix -- prefixed
    | DeclarationsBlock -- basic
  
  /* -------------------------------------------------------------------------- */
  /* property name                                                              */
  /* -------------------------------------------------------------------------- */
  
  /* 
    @diff
      - collectionKeyword
  */
  propertyName := kebabCase
  
  /* -------------------------------------------------------------------------- */
  /* property Value                                                             */
  /* -------------------------------------------------------------------------- */
  
  /*
    @diff
      - pascalCase
  */
  PropertyValue := propsFunc | PropertyValueFunc | kebabCase | Formula
}