// Combination of Atomic and Molecule

Primitive <: Atomic {
  Formula = 
    | WrapTerm Apply+ -- expression
    | numeral
  Apply = operator WrapTerm
  WrapTerm = 
    | "(" AtomicFormula ")" -- expression
    | numeral
  AtomicFormula = numeral (operator numeral)+
  numeral = 
    | numeralWithUnit 
    | generatedNumber
  numeralWithUnit = generatedNumber unit
  
  generatedLiteral = props | literal
  literal = "\"" (~"\"" any)* "\""
}