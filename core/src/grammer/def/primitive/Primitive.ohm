// Combination of Atomic and Molecule

Primitive <: Molecule {
  Primitive = organism | molecule | wrapAtomic | atomic
  
  organism = formula
  
  formula = formulaElements (mathOperator formulaElements)*
  formulaElements = 
    | numeral -- number
    | "(" atomicFormula ")" -- expression
  atomicFormula = numeral (mathOperator numeral)+
  numeral = numeralWithUnit | number | propsFunc
}