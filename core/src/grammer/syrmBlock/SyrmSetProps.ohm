SyrmSetProps <: SyrmCascade {
  /* root */
  Root := "<SetProps>" space* Block* space* "</SetProps>"
  
  /* -------------------------------------------------------------------------- */
  /* block                                                                      */
  /* -------------------------------------------------------------------------- */
  
  /* 
    @diff
      - DeclarationsBlockWithPrefix DeclarationsBlock* syrmSuffixAnnotation 
  */
  Block := 
    | DeclarationsBlockWithPrefix syrmBetweenAnnotation DeclarationsBlock -- between
    | DeclarationsBlockWithPrefix -- prefixed
    | DeclarationsBlock -- basic
  
  /* -------------------------------------------------------------------------- */
  /* property name                                                              */
  /* -------------------------------------------------------------------------- */
  
  /* 
    @diff
      - collectionKeyword
      - kebabCase
      + camelCase
  */
  propertyName := camelCase 
  
  /* -------------------------------------------------------------------------- */
  /* property Value                                                             */
  /* -------------------------------------------------------------------------- */
  
  /*
    @diff
      - pascalCase
      - kebabCase
      + camelCase
      - PropertyValueFunc
  */
  PropertyValue := propsFunc | camelCase | Formula
  
  /* -------------------------------------------------------------------------- */
  /* Selector                                                                   */
  /* -------------------------------------------------------------------------- */
  
  /*
    @diff
      - basicSelector PseudoSelector
      - PseudoSelector
  */
  AtomicSelector := basicSelector
  
  /* 
    @diff
      - attributeSelector
      - htmlTagSelector
      + jsxTagSelector
  */
  basicSelector := jsxTagSelector | rootSelector | universalSelector
}